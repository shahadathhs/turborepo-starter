{
  "name": "express-rest-api",
  "version": "0.0.1",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "start": "node ./dist/server.js",
    "typecheck": "tsc --noEmit && tsc-alias",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "format:check": "prettier --ignore-path .prettierignore --check \"**/*.+(js|ts|json)\"",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "This is a Express REST API project with TypeScript, Node.js, Express, and MongoDB.",
  "dependencies": {
    "bcryptjs": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.2",
    "ms": "^2.1.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.9.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
